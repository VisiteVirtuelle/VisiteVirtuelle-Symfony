{% extends ['base.html.twig'] %}

{% block title %}{{ parent() }} - {{ 'admin.dashboard.title'|trans }}{% endblock %}

{% block body %}
    <div class="row">
        <div class="col-2">
            <ul class="navbar-nav">
                {% for group in groups %}
                    <li>
                        <a href=urls>
                            {{ group.name }}
                        </a>
                    </li>
                {% else %}
                    <li>C'est vide</li>
                {% endfor %}
            </ul>
        </div>

        <div class="col">
            {% include template  %}
        </div>
    </div>
{% endblock %}