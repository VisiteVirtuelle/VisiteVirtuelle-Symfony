{% extends 'base.html.twig' %}

{% block main %}
    {{ form_start(form) }}
        {{ form_row(form.fullName) }}
        {{ form_row(form.username) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password.first) }}
        {{ form_row(form.password.second) }}

        <button type="submit">{{ 'security.register.submit'|trans }}</button>
    {{ form_end(form) }}
{% endblock %}
